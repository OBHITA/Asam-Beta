<?xml version="1.0"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>

  <appSettings>
    <add key="NHibernateDatabaseCatalogName"
         value="AsamDemo"
         xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NHibernateDatabaseServerName"
         value="OBHITADEMODB02"
         xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>

  <system.identityModel>
    <identityConfiguration>
      <issuerNameRegistry>
        <trustedIssuers xdt:Transform="Replace">
          <add thumbprint="25479124E9043F659816021AF4CD89ECA2C42209"
          name="https://sts-demo.feisystems.com/issue/wsfed"/>
        </trustedIssuers>
      </issuerNameRegistry>
    </identityConfiguration>
  </system.identityModel>

  <system.identityModel.services>
    <federationConfiguration xdt:Transform="Replace">
      <cookieHandler requireSsl="false" />
      <wsFederation passiveRedirectEnabled="true" issuer="https://sts-demo.feisystems.com/issue/wsfed" realm="https://asam-demo.feisystems.com/" requireHttps="false" />
    </federationConfiguration>
  </system.identityModel.services>

  <system.identityModel>
    <identityConfiguration>
      <audienceUris  xdt:Transform="Replace">
        <add value="https://asam-demo.feisystems.com" />
        <add value="https://asam-demo.feisystems.com/api" />
      </audienceUris>
    </identityConfiguration>
  </system.identityModel>

</configuration>